print ('How is the weather?')
weather = input()
if weather == 'Sunny':
    print ('Wear a cap.')
    print ('Wear sunglasses.')
if weather == 'Rainy':
    print ('Wear a raincoat.')
    print ('Carry an umbrella.')
if weather == 'Snowy':
    print ('Wear a sweatshirt')
else:
    print ('Wear a T-Shirt')